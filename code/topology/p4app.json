      "auto-control-plane": true,
      "cli": true,
      "pcap_dump": true,
      "bmv2_log": true,
      "links": [["h1", "s1"], ["s1", "s2"], ["s1", "s3"], 
                ["h2", "s4"], ["s2", "s4"], ["s3", "s4"]],
      "hosts": {
        "h1": {},
        "h2": {}
      },
      "switches": {
        "s1": { "entries" : "topology/s1-commands.txt" },
        "s2": { "entries" : "topology/s2-commands.txt" },
        "s3": { "entries" : "topology/s3-commands.txt" },
        "s4": { "entries" : "topology/s4-commands.txt" }
      },
      "configs": {
        "s1": "s1.p4",
        "s2": "s2.p4",
        "s3": "s3.p4",
        "s4": "s4.p4"
      }
    }
  }
}